#include "rv1106-amp.dtsi"
#include <dt-bindings/input/input.h>

/ {
	/**********KEYBOARD**********/
	gpio-keys {
		compatible = "gpio-keys";
		label = "keypad";
		autorepeat;

		reset {
			label = "Reset";
			linux,code = <1>; // KEY_ESC
			gpios = <&gpio1 RK_PC6 GPIO_ACTIVE_LOW>;
			debounce-interval = <5>;
			wakeup-source;
		};

		up {
			label = "Up";
			linux,code = <103>; // KEY_UP
			gpios = <&gpio1 RK_PD0 GPIO_ACTIVE_LOW>;
			debounce-interval = <5>;
			wakeup-source;
		};

		down {
			label = "Down";
			linux,code = <108>; // KEY_DOWN
			gpios = <&gpio1 RK_PD1 GPIO_ACTIVE_LOW>;
			debounce-interval = <5>;
			wakeup-source;
		};

		left {
			label = "Left";
			linux,code = <105>; // KEY_LEFT
			gpios = <&gpio1 RK_PD2 GPIO_ACTIVE_LOW>;
			debounce-interval = <5>;
			wakeup-source;
		};

		right {
			label = "Right";
			linux,code = <106>; // KEY_RIGHT
			gpios = <&gpio1 RK_PD3 GPIO_ACTIVE_LOW>;
			debounce-interval = <5>;
			wakeup-source;
		};
	};
};

